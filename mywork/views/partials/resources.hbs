{{#if manifest}}
  {{#each this.css}}
      <link rel="stylesheet" type="text/css" href="{{../root}}{{this}}">
  {{/each}}

  {{#each this.js}}
      <script type="text/javascript" src="{{../root}}{{this}}"></script>
  {{/each}}
{{else}}
  <link rel="stylesheet" href="/dist/app.css" /> 
  <script type="text/javascript" src="/dist/vendor.common.js"></script>
  <script type="text/javascript" src="/dist/app2.bundle.js"></script>
  <script type="text/javascript" src="/dist/app.bundle.js"></script>
{{/if}}